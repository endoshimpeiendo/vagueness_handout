@article{Nolan2013,
abstract = {Philosophers have found postulating possible worlds to be very useful in a number of areas, including philosophy of language and mind, logic, and metaphysics. Impossible worlds are a natural extension to this use of possible worlds, and can help resolve a number of difficulties thrown up by possible-worlds frameworks. Philosophers spend a lot of time thinking about what is possible or necessary, as well as about what is in fact the case. Many philosophers doing metaphysics are interested in what features of our world are metaphysically necessary, and what are mere contingencies. Logicians are interested in what arguments are necessarily truth preserving, not just in what arguments happen to have all true premises and a true conclusion. Moral and political philosophers spend a lot of time considering hypothetical scenarios or societal arrangements. Possibility and necessity seem important to philosophers, and this is one reason why talk about modality (possibility and necessity) has played an important role in many philosophical debates in the last half-century. There has been a widespread use of possible worlds in philosophical discussions. But what lies beyond the possible? Is there a realm of impossibility, and impossible worlds? On the face of it, we appear to be able to describe impossible situations, and moreover to produce a range of different scenarios that we take to be completely impossible. We do this for a number of reasons. Impossible circumstances are the subject of stories and other artwork, like the drawings of impossibilities by M.C. Escher or fictions like some of Jorge Luis Borges's short stories. Another less high-brow kind of example of impossible fictions can arguably be found in many time-travel stories and films: even if time travel of some sort is possible, genuine time travel arguably cannot work as it was portrayed in the Back to the Future movies. Description of impossible scenarios seems to serve more theoretical purposes as well. Sometimes impossible scenarios are described intentionally, but sometimes people end up dealing with impossibilities despite intending to only describe possibilities. Many claims of interest to philosophers seem to be necessary if true, and impossible if false. Central metaphysical claims about the nature of the world are often supposed to have this feature, such as ''analyses'' of causation, or hypotheses about the nature of laws of nature, or claims about the metaphysics of possible worlds; philosophers who offer analyses of knowledge or meaning or free will often take themselves to be establishing conceptual truths that would be absolutely necessary; and the fundamental principles of ethics are often supposed to be like this too, just to take three kinds of examples. When subject matters like this are under dispute, chances are that one or more parties to a disagreement believe a theory of the world that is, unbeknownst to them, entirely impossible. Despite the impossibility of many false philosophical positions, those positions seem to be understandable, and the sorts of things we can have rational disagreements about. In Philosophy Compass 8/4 (2013): 360–372, 10.1111/phc3.12027},
author = {Nolan, Daniel P},
doi = {10.1111/phc3.12027},
file = {:home/shimpei/Documents/Mendeley//Nolan - 2013 - Impossible Worlds.pdf:pdf},
journal = {Philosophy Compass},
number = {4},
pages = {360--372},
title = {{Impossible Worlds}},
volume = {8},
year = {2013}
}
