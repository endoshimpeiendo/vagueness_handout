@article{Miller2001a,
abstract = {This paper is a survey of current work in the metaphysics of possibility. Rather than feigning neutrality, which critical readers would not believe, I prefer to make my point of view clear from the outset. I am a Modal Realist, who was written in defense of the reality of possible worlds in the past. In this paper I continue to argue for a version of that position in the course of my survey of the literature. Whereas the problems with Linguistic Ersatzism and Fictionalism are deep and insoluble, the main flaw of Modal Realism, its ontological extravagance, can be completely eliminated without any loss of explanatory power. Before preceding to my arguments there are two features of the continuing debate over the reality of possible worlds to which I would like to draw the reader's attention. The first is its personalized character. Other theories of modality are seen as options available to all, but Modal Realism is identified with David Lewis. When a philosopher of Lewis' stature propounds a theory and he is the only significant philosopher who defends it, it is natural that other philosophers will think of it as his theory {\~{}}. This tendency should be resisted however. To look at a philosophical position through the lens of a single mind, no matter how brilliant, gives a distorted picture. Lewis has highly articulated and well argued views on a host of metaphysical issues from nominalism to personal identity. As valuable as it is, the very power of his philosophical vision makes it harder to see Modal Realism as a way of understanding possibility consistent with a whole spectrum of metaphysical positions. How would Modal Realism look if developed by philosophers who disagreed with Lewis about nominalism, personal identity, minds, reductionism, or materialism? Such interesting and potentially fruitful questions go unasked when we persist in thinking of Modal Realism as the idiosyncrasy of one brilliant mind. Modal Realism is provocative in its ontological extravagance. This, the second feature of the debate to which I draw attention, has surely not gone unnoticed, but its connection to the first has. In this paper I propose to examine Modal Realism as an option for dealing with possibility that is compatible with a wide range of views on other issues in metaphysics. I will show how the ontological extravagance of Modal Realism varies with the specific form it takes. Moreover, I will trace the ontological extravagance in Lewis's position to what I consider an inessential quirk --his extending Modal Realism to the outer realm of possibility. In conclusion I will excise the quirk showing how its job can be done more economically by other means and so propose a downsized Modal Realism as modest as any Actualism.},
author = {Miller, Richard B},
doi = {10.1007/BF02379767},
file = {:home/shimpei/Documents/Mendeley/Miller - 2001 - Moderate Modal Realism.pdf:pdf},
journal = {Philosophia},
number = {3},
pages = {3----38},
title = {{Moderate Modal Realism}},
url = {https://doi.org/10.1007/BF02379767},
volume = {28},
year = {2001}
}
