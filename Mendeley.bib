@article{Bricker2006a,
abstract = {According to David Lewis, a realist about possible worlds must hold that actuality is relative: the worlds are ontologically all on a par; the actual and the merely possible differ, not absolutely, but in how they relate to us. Call this 'Lewisian realism'. The alternative, 'Leibnizian realism', holds that actuality is an absolute property that marks a distinction in ontological status. Lewis presents two arguments against Leibnizian realism. First, he argues that the Leibnizian realist cannot account for the contingency of actuality. Second, he argues that the Leibnizian realist cannot explain why skepticism about one's own actuality is absurd. In this paper, I mount a defense of Leibnizian realism.},
author = {Bricker, Phillip},
