@article{Perszyk1993,
abstract = { Extended modal realism is David Lewis's realism about possible worlds and their inhabitants extended to impossible worlds and their inhabitants. Takashi Yagisawa has given the most serious defence in
 print of the conditional thesis that if Lewisian modal realism is to be accepted, then extended modal realism is to be accepted.' He has two arguments for this thesis, what I shall call 'the Parallel-Case Argument' and 'the Theoretical-Benefits Argument'. A central issue in the metaphysics of modality is whether Yagisawa's thesis is right. Few philosophers have been (or will be) able to face impossibilia with much courage; most already reject Lewisian modal realism. But if Yagisawa is right, not only would Lewis's critics have further reason to reject his modal realism - its ontology is even crazier than they thought - but (more importantly) Lewis himself, insofar as he unequivocally rejects genuine impossible worlds and their inhabitants, would have good reason to think that whatever benefits of modal realism there may be just aren't worth its ontological cost after all. My aim in this paper is to reject Yagisawa's thesis by rebutting his two arguments in support.},
author = {Perszyk, Kenneth J.},
doi = {10.2307/30226497},
file = {:home/shimpei/Documents/Mendeley/Perszyk - 1993 - Against Extended Modal Realism.pdf:pdf},
journal = {Journal of Philosophical Logic},
